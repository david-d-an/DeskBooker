@page
@model DeptManagerModel
@{
  ViewData["Title"] = "Department Managers";
}

<h1 class="display-4">Department Manager pulled from MySql</h1>


<hr />

<table class="table table-striped table-bordered dt-responsive nowrap">
  <thead class="thead-light">
    <tr>
      <th>Dept ID</th>
      <th>Emp ID</th>
      <th>From Date</th>
      <th>To Date</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var dm in Model.deptManager)
    {
      <tr>
        <td>@dm.DeptNo</td>
        <td>@dm.EmpNo</td>
        <td>@dm.FromDate</td>
        <td>@dm.ToDate</td>
      </tr>
    }
  </tbody>
</table>


